/**
 * For more details on how to configure Wrangler, refer to:
 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "ai-mcp",
  "main": "src/index.ts",
  "compatibility_date": "2025-08-23",
  "observability": {
    "enabled": true
  },
  "keep_vars": true,
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": [
        "SimplePromptAgent"
      ]
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "AGENT",
        "class_name": "SimplePromptAgent"
      }
    ]
  },
  "vars": {
    "AI_PROVIDER": "google",
    "MODEL_ID": "gemini-2.5-flash",
    "AI_PROVIDER_API_KEY": "AIzaSyCVZj3LkrA7PISrXQzrFbEvZG2rGSVk7Ec"
  },
  "env": {
    "staging": {
      "durable_objects": {
        "bindings": [
          {
            "name": "AGENT",
            "class_name": "SimplePromptAgent"
          }
        ]
      },
      "services": [
        {
          "binding": "MCP_SERVICE",
          "service": "mcp"
        }
      ],
      "vars": {
        "AI_PROVIDER": "google",
        "MODEL_ID": "gemini-2.5-flash",
        "AI_PROVIDER_API_KEY": "AIzaSyCVZj3LkrA7PISrXQzrFbEvZG2rGSVk7Ec"
      },
    }
  },
}